<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_ILogService" />
                <binding name="BasicHttpBinding_IMathService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:41397/LogService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_ILogService" contract="LogServerService.ILogService"
                name="BasicHttpBinding_ILogService" />
            <endpoint address="http://localhost:41397/MathService.svc" binding="basicHttpBinding"
                bindingConfiguration="BasicHttpBinding_IMathService" contract="MathServerService.IMathService"
                name="BasicHttpBinding_IMathService" />
        </client>
    </system.serviceModel>
</configuration>